{
  "name": "alpha-cli-icons",
  "version": "1.0.4",
  "description": "svg icons transformer.",
  "author": "Jun",
  "license": "MIT",
  "scripts": {
    "generate": "node scripts/transform.js",
    "build:bundle": "sudo rm -rf dist && node_modules/.bin/rollup --c ./scripts/rollup.config.bundle.js",
    "update-version": "npm version patch -m 'update version'",
    "pub": "rm -rf publish && mkdir publish && cp -r {icons,components,package.json} publish && cd publish && npm publish"
  },

  "devDependencies": {
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "glob": "^7.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "svgo": "^1.3.0",
    "@rollup/plugin-babel": "^5.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "rollup": "^1.19.4"
  }
}
